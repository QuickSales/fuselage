{
  "version": 2,
  "projects": {
    "..-playground-message-parser-playground": {
      "root": "packages/../playground/message-parser-playground",
      "sourceRoot": "packages/../playground/message-parser-playground/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/playground/message-parser-playground",
            "index": "playground/message-parser-playground/src/index.html",
            "main": "playground/message-parser-playground/src/main.tsx",
            "polyfills": "playground/message-parser-playground/src/polyfills.ts",
            "tsConfig": "playground/message-parser-playground/tsconfig.app.json",
            "assets": [
              "playground/message-parser-playground/src/favicon.ico",
              "playground/message-parser-playground/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "playground/message-parser-playground/src/environments/environment.ts",
                  "with": "playground/message-parser-playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "..-playground-message-parser-playground:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "..-playground-message-parser-playground:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "packages/../playground/message-parser-playground/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/playground/message-parser-playground"],
          "options": {
            "jestConfig": "playground/message-parser-playground/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "@rocket.chat/css-in-js": {
      "root": "packages/css-in-js"
    },
    "@rocket.chat/css-supports": {
      "root": "packages/css-supports"
    },
    "@rocket.chat/emitter": {
      "root": "packages/emitter"
    },
    "@rocket.chat/eslint-config-alt": {
      "root": "packages/eslint-config-alt"
    },
    "@rocket.chat/fuselage": {
      "root": "packages/fuselage"
    },
    "@rocket.chat/fuselage-hooks": {
      "root": "packages/fuselage-hooks"
    },
    "@rocket.chat/fuselage-polyfills": {
      "root": "packages/fuselage-polyfills"
    },
    "@rocket.chat/fuselage-tokens": {
      "root": "packages/fuselage-tokens"
    },
    "@rocket.chat/fuselage-ui-kit": {
      "root": "packages/fuselage-ui-kit"
    },
    "@rocket.chat/icons": {
      "root": "packages/icons"
    },
    "@rocket.chat/memo": {
      "root": "packages/memo"
    },
    "@rocket.chat/message-parser": {
      "root": "packages/message-parser"
    },
    "@rocket.chat/mp3-encoder": {
      "root": "packages/mp3-encoder"
    },
    "@rocket.chat/onboarding-ui": {
      "root": "packages/onboarding-ui"
    },
    "@rocket.chat/peggy-loader": {
      "root": "packages/peggy-loader"
    },
    "@rocket.chat/prettier-config": {
      "root": "packages/prettier-config"
    },
    "@rocket.chat/string-helpers": {
      "root": "packages/string-helpers"
    },
    "@rocket.chat/styled": {
      "root": "packages/styled"
    },
    "@rocket.chat/stylis-logical-props-middleware": {
      "root": "packages/stylis-logical-props-middleware"
    },
    "@rocket.chat/ui-kit": {
      "root": "packages/ui-kit"
    },
    "scripts": {
      "root": "scripts"
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none"
      },
      "library": {
        "style": "none",
        "linter": "eslint"
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "..-playground-message-parser-playground"
}
